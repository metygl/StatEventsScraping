{
  "rewrites": [
    { "source": "/", "destination": "/output/events.html" },
    { "source": "/export", "destination": "/output/export.html" },
    { "source": "/status", "destination": "/output/status.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=3600, stale-while-revalidate" }
      ]
    }
  ]
}
